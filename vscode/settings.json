                {
                    "key": "b",
                    "name": "Found in all buffers",
                    "icon": "files",
                    "type": "command",
                    "command": "workbench.action.showAllEditorsByMostRecentlyUsed"
                },
                {
                    "key": "r",
                    "name": "Reveal in explorer",
                    "icon": "files",
                    "type": "command",
                    "command": "revealInExplorer"
                }
            ]
        },
        // #endregion
        // #region Debug
        {
            "key": "d",
            "name": "Debug...",
            "icon": "bug",
            "type": "bindings",
            "bindings": [
                {
                    "key": "r",
                    "name": "Save and Run Debugging",
                    "icon": "debug-start",
                    "type": "command",
                    "commands": [
                        "workbench.action.files.save",
