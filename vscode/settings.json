        },
        {
            "before": [
                "<leader>",
                "f",
                "d"
            ],
            "commands": [
                "editor.action.formatDocument"
            ],
            "args": [
                {
                    "insertSpaces": true,
                    "tabSize": 4
                }
            ]
        },
